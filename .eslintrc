{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "plugins": [
    "flow-vars"
  ],
  "rules": {
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1,
    "quotes": [
      2,
      "double",
      "avoid-escape"
    ],
    "no-var": 2,
    "space-after-keywords": 2,
    "strict": 0,
    "no-underscore-dangle": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "new-cap": 0,
    "no-loop-func": 0,
    "no-unreachable": 0,
    "no-labels": 0,
    "no-process-exit": 0,
    "camelcase": 0,
    "no-console": 0,
    "no-constant-condition": 0,
    "no-inner-declarations": 0,
    "semi": [2, "always"]
  },
  "env": {
    "node": true
  }
}
