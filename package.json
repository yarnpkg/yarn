{
  "name": "yarn",
  "installationMethod": "unknown",
  "version": "1.6.0-0",
  "license": "BSD-2-Clause",
  "preferGlobal": true,
  "description": "📦🐈 Fast, reliable, and secure dependency management.",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bytes": "^3.0.0",
    "camelcase": "^5.0.0",
    "chalk": "^2.1.0",
