    "commander": "^2.9.0",
    "death": "^1.0.0",
    "debug": "^2.2.0",
    "detect-indent": "^5.0.0",
    "glob": "^7.1.1",
    "gunzip-maybe": "^1.4.0",
    "ini": "^1.3.4",
    "inquirer": "^3.0.1",
    "invariant": "^2.2.0",
    "is-builtin-module": "^1.0.0",
    "is-ci": "^1.0.10",
    "leven": "^2.0.0",
    "loud-rejection": "^1.2.0",
    "micromatch": "^2.3.11",
    "mkdirp": "^0.5.1",
    "node-emoji": "^1.6.1",
    "object-path": "^0.11.2",
    "proper-lockfile": "^2.0.0",
    "read": "^1.0.7",
    "request": "^2.81.0",
    "request-capture-har": "^1.2.2",
    "rimraf": "^2.5.0",
    "semver": "^5.1.0",
    "strip-bom": "^3.0.0",
