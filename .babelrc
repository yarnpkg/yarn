{
  "only": "*.js",
  "env": {
    "pre-node5": {
      "presets": [
        ["env", {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }],
        "flow",
        "stage-0"
      ],
      "plugins": [
        ["array-includes"],
        ["transform-inline-imports-commonjs"],
        ["transform-runtime", { "polyfill": true, "regenerator": false }]
      ]
    },
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }],
        "flow",
        "stage-0"
      ],
      "plugins": [
        ["transform-inline-imports-commonjs"],
        ["transform-runtime", { "polyfill": true, "regenerator": false }]
      ]
    }
  },
  "presets": [
      ["env", {
        "targets": {
          "node": "current"
        },
        "loose": true
      }],
      "flow",
      "stage-0"
  ],
  "plugins": [
    ["transform-inline-imports-commonjs"],
    ["transform-runtime", { "polyfill": false, "regenerator": true }]
  ]
}
