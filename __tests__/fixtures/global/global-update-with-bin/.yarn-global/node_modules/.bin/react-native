#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../.yarn-global/node_modules/react-native-cli/index.js" "$@"
  ret=$?
else 
  node  "$basedir/../.yarn-global/node_modules/react-native-cli/index.js" "$@"
  ret=$?
fi
exit $ret
