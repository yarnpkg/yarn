age/babel-preset-es2015-node\n","maintainers":[{"name":"rtsao","email":"ryan.j.tsao@gmail.com"}],"time":{"modified":"2016-08-25T22:15:08.493Z","created":"2015-10-31T09:23:47.905Z","5.0.0":"2015-10-31T09:23:47.905Z","5.0.1":"2015-10-31T09:34:11.657Z","4.0.0":"2016-01-08T05:49:49.493Z","5.0.2":"2016-01-08T05:50:36.273Z","4.0.1":"2016-03-08T14:57:14.294Z","5.0.3":"2016-03-08T14:58:37.870Z","6.0.0":"2016-04-26T20:21:03.980Z","5.0.4":"2016-04-26T20:27:02.191Z","4.0.2":"2016-04-26T20:30:23.596Z","5.0.5":"2016-04-26T20:31:51.037Z","6.0.1":"2016-04-26T20:36:02.737Z","6.1.0":"2016-06-09T02:12:15.692Z","6.1.1":"2016-08-25T22:15:08.493Z"},"homepage":"https://babeljs.io/","repository":{"type":"git","url":"git+https://github.com/rtsao/babel-preset-es2015-node.git"},"author":{"name":"Ryan Tsao","email":"ryan.j.tsao@gmail.com"},"bugs":{"url":"https://github.com/rtsao/babel-preset-es2015-node/issues"},"license":"MIT","readmeFilename":"README.md","users":{"tregusti":true,"theaklair":true},"_attachments":{}}